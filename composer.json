{
  "name": "wpify/wpify",
  "type": "wordpress-plugin",
  "require": {
    "composepress/core": "0.9.0.0",
    "doctrine/collections": "^1.6",
    "cmb2/cmb2": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Wpify\\": "src\\"
    },
    "files": [
      "vendor/cmb2/cmb2/init.php"
    ]
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "phpcs": "phpcs --standard=phpcs.xml"
  }
}
