{
  "name": "wpify/plugin-skeleton",
  "description": "WPify Plugin Skeleton",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "config": {
	"platform-check": false
  },
  "autoload": {
	"psr-4": {
	  "WpifyPluginSkeleton\\": "src/"
	}
  },
  "scripts": {
	"post-create-project-cmd": "@composer replace-name"
  },
  "require": {
	"php": ">=7.3.0||^8.0.0",
	"ext-json": "*"
  },
  "require-dev": {
	"wpify/plugin-composer-scripts": "^1.1",
	"wpify/scoper": "^2.2",
	"wpify/tracy": "^1.0"
  },
  "extra": {
	"wpify-scoper": {
	  "prefix": "WpifyPluginSkeletonDeps"
	}
  }
}
