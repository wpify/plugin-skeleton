{
  "name": "wpify/plugin",
  "description": "",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "scripts": {
	"phpcs": "phpcs --standard=WordPress",
	"phpcbf": "phpcbf --standard=WordPress"
  },
  "extra": {
	"wordpress-scoper": {
	  "prefix": "WpifyPluginDeps",
	  "folder": "deps",
	  "globals": ["wordpress", "woocommerce"],
	  "packages": {
		"wpify/core": "^4.2",
		"doctrine/collections": "^1.6",
		"wpify/tools": "^2.3"
	  }
	}
  },
  "autoload": {
	"psr-4": {
	  "WpifyPlugin\\": "src/"
	}
  },
  "require": {
	"php": ">=7.3.0||^8.0.0",
	"cmb2/cmb2": "^2.9"
  },
  "require-dev": {
	"squizlabs/php_codesniffer": "^3.5",
	"wpify/cli": "^0.3",
	"wp-coding-standards/wpcs": "^2.3",
	"dealerdirect/phpcodesniffer-composer-installer": "^0.7"
  }
}
