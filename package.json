{
	"name": "wpify-plugin-skeleton",
	"version": "1.0.0",
	"main": "assets/plugin.js",
	"scripts": {
		"build": "wp-scripts build --config webpack.config.js",
		"check-engines": "wp-scripts check-engines --config webpack.config.js",
		"check-licenses": "wp-scripts check-licenses --config webpack.config.js",
		"format": "wp-scripts format --config webpack.config.js",
		"lint:css": "wp-scripts lint-style --config webpack.config.js",
		"lint:js": "wp-scripts lint-js --config webpack.config.js",
		"lint:md:docs": "wp-scripts lint-md-docs --config webpack.config.js",
		"lint:md:js": "wp-scripts lint-md-js --config webpack.config.js",
		"lint:pkg-json": "wp-scripts lint-pkg-json --config webpack.config.js",
		"packages-update": "wp-scripts packages-update --config webpack.config.js",
		"start": "wp-scripts start --config webpack.config.js",
		"test:e2e": "wp-scripts test-e2e --config webpack.config.js",
		"test:unit": "wp-scripts test-unit-js --config webpack.config.js"
	},
	"author": "WPify <info@wpify.io>",
	"license": "ISC",
	"engines": {
		"node": ">=v14.17.6"
	},
	"sideEffects": [
		"*.scss"
	],
	"browserslist": [
		"defaults",
		"cover 97%"
	],
	"devDependencies": {
		"@wordpress/scripts": "^18.0.1"
	}
}
