{
  "name": "wpify",
  "version": "0.1.1",
  "main": "assets/plugin.jsx",
  "scripts": {
    "start": "gulp start",
    "build": "gulp build"
  },
  "author": "Daniel Mejta <daniel@mejta.net>",
  "license": "ISC",
  "sideEffects": [
    "*.scss"
  ],
  "browserslist": [
    "defaults",
    "cover 97%"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react-hooks",
      "babel",
      "react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "useJSXTextNode": true,
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "jsx": true
      }
    },
    "globals": {
      "document": false,
      "wp": false,
      "__webpack_public_path__": true,
      "WPIFY": false
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".json"
          ]
        }
      },
      "react": {
        "version": "v16.9.0"
      }
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-css-modules"
    ],
    "plugins": [
      "stylelint-scss",
      "stylelint-order"
    ],
    "rules": {
      "at-rule-no-unknown": null,
      "scss/at-rule-no-unknown": true,
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^custom-/"
          ]
        }
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {},
      "postcss-font-display": {
        "display": "swap",
        "replace": false
      },
      "postcss-pxtorem": {}
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.7.0",
    "@wordpress/scripts": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-codeframe-formatter": "^1.0.2",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-notifier": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-font-display": "^0.1.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.2",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-formatter-pretty": "^2.0.1",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-livereload-plugin": "^2.3.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.4",
    "@wordpress/block-editor": "^4.1.0",
    "@wordpress/blocks": "^6.18.0",
    "@wordpress/dom-ready": "^2.9.0",
    "@wordpress/i18n": "^3.13.0",
    "classnames": "^2.2.6",
    "custom-event": "^1.0.1",
    "event-propagation-path": "^1.0.5",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  }
}
